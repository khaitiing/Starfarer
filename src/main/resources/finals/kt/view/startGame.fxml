<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="finals.kt.view.startGameController">
   <children>
      <ImageView fitHeight="613.0" fitWidth="804.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/startGamebg.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="spaceshipImageView" fitHeight="60.0" fitWidth="30.0" layoutX="12.0" layoutY="528.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="20.0" layoutY="19.0" mnemonicParsing="false" onAction="#backtoMenu" prefHeight="44.0" prefWidth="95.0" text="End Game">
         <font>
            <Font name="Agency FB Bold" size="20.0" />
         </font>
      </Button>
      <Label fx:id="timeLabel" layoutX="655.0" layoutY="27.0" text="TIMER" textFill="WHITE">
         <font>
            <Font name="Agency FB Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="pointsLabel" layoutX="655.0" layoutY="70.0" prefHeight="29.0" prefWidth="129.0" text="POINTS" textFill="WHITE">
         <font>
            <Font name="Agency FB Bold" size="24.0" />
         </font>
      </Label>
      <ImageView fx:id="life1" fitHeight="33.0" fitWidth="25.0" layoutX="660.0" layoutY="111.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/life.png" />
         </image>
      </ImageView>
      <ImageView fx:id="life2" fitHeight="150.0" fitWidth="25.0" layoutX="700.0" layoutY="111.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/life.png" />
         </image>
      </ImageView>
      <ImageView fx:id="life3" fitHeight="150.0" fitWidth="25.0" layoutX="740.0" layoutY="111.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/life.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
